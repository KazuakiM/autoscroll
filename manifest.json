{
    "background": {"scripts": ["src/background.js"]},
    "browser_action": {
        "default_icon": "img/browser_action_32.png",
        "default_title": "AutsSroller"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["src/content_scripts.js"]
    }],
    "description": "AutsSroller",
    "homepage_url": "https://github.com/KazuakiM/autoscroller",
    "icons": {
        "48": "img/icons_48.png",
        "96": "img/icons_48.png"
    },
    "manifest_version": 2,
    "name": "AutsSroller",
    "options_ui": {"page": "src/options_ui.html"},
    "permissions": [
        "activeTab",
        "storage"
    ],
    "version": "1.0"
}
